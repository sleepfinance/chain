{
  "name": "@sleepfinance/chain",
  "version": "0.1.1",
  "description": "Sleepfinance Chains",
  "keywords": [
    "sleep.finance",
    "chain"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sushiswap/sushiswap.git",
    "directory": "packages/validate"
  },
  "license": "MIT",
  "author": "Matthew Lilley <hello@matthewLilley.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "./index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup index.ts --format esm,cjs --watch --dts",
    "generate": "ls chains.ts || ts-node generate.ts",
    "preinstall": "pnpm run generate",
    "lint": "TIMING=1 eslint ./index.ts --fix",
    "test": "jest --passWithNoTests"
  },
  "jest": {
    "preset": "@sushiswap/jest-config/node"
  },
  "devDependencies": {
    "@sushiswap/eslint-config": "^0.1.1",
    "@sushiswap/jest-config": "^0.1.0",
    "@sushiswap/prettier-config": "0.1.0",
    "@sushiswap/typescript-config": "^0.1.0",
    "@types/jest": "^29.2.0",
    "@types/node-fetch": "^2.6.2",
    "eslint": "8.29.0",
    "jest": "29.3.1",
    "node-fetch": "^2.6.2",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  }
}
